<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/screen"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Highscores">

    <LinearLayout
        android:id="@+id/ljudLayout"
        android:layout_width="100dp"
        android:layout_height="600dp"
        android:layout_marginTop="30dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toStartOf="@+id/vibLayout"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/visLayout"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/ljudScoresText"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Ljud"
            android:textColor="#000000"
            android:textSize="22sp" />

        <TextView
            android:id="@+id/ljudScores"
            android:layout_width="match_parent"
            android:layout_height="557dp"
            android:text="1. test"
            android:textColor="#000000"
            android:textSize="16sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/visLayout"
        android:layout_width="100dp"
        android:layout_height="600dp"
        android:layout_marginTop="30dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toStartOf="@+id/ljudLayout"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/visualScoreText"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Visuellt"
            android:textColor="#000000"
            android:textSize="22sp" />

        <TextView
            android:id="@+id/visScores"
            android:layout_width="match_parent"
            android:layout_height="557dp"
            android:text="1. test"
            android:textColor="#000000"
            android:textSize="16sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/moveLayout"
        android:layout_width="100dp"
        android:layout_height="600dp"
        android:layout_marginTop="30dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/vibLayout"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/movementScoresText"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="RÃ¶relse"
            android:textColor="#000000"
            android:textSize="22sp" />

        <TextView
            android:id="@+id/moveScores"
            android:layout_width="match_parent"
            android:layout_height="557dp"
            android:text="1. test"
            android:textColor="#000000"
            android:textSize="16sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/vibLayout"
        android:layout_width="100dp"
        android:layout_height="600dp"
        android:layout_marginTop="30dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toStartOf="@+id/moveLayout"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/ljudLayout"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/vibrationScoresText"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Vibration"
            android:textColor="#000000"
            android:textSize="22sp" />

        <TextView
            android:id="@+id/vibScores"
            android:layout_width="match_parent"
            android:layout_height="557dp"
            android:text="1. test"
            android:textColor="#000000"
            android:textSize="16sp" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>